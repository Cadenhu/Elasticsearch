<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.exampl.estemplate.springdateelasticsearch.repository.BuildingMapper">
<select id="findAll" resultType="java.util.Map">
    SELECT
        id
        ,building_id
        ,dataln_building_id
        ,dataln_building_unit_id
        ,dataln_presale_id
        ,unit_name
        ,layers
        ,sets
        ,tags
        ,left_margin
        ,top_margin
    FROM building_unit limit 0,5000
</select>
</mapper>